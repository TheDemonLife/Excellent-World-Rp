    }
    plafk[playerid] = 0;
    }
	if(!IsPlayerConnected(playerid)) return true;
	if(GetPlayerWeapon(playerid) == 38 && PlayerInfo[playerid][pAdmInections] < 7) KickEx(playerid);
	if(!IsPlayerInAnyVehicle(playerid) && PlayerInfo[playerid][pAdmInections] < 7)
	{
		new Float:Z;
		GetPlayerPos(playerid,Z,Z,Z);
		new anim = GetPlayerAnimationIndex(playerid);
		if((anim == 1538 || anim == 1539 || anim == 1543) && Z > 2)
		{
			if(!NotFlyingHack(playerid))
			{
				format(stringer,sizeof(stringer),"[Античит]: %s был кикнут по подозрению в читерстве. (Полёт)",PlayerInfo[playerid][pNames]);
