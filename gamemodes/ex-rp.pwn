		}
	}
	if(IsPlayerInAnyVehicle(playerid))
	{
		if(GetPlayerVehicleID(playerid) != GetPVarInt(playerid,"ac_car_id"))
		{
			SetPVarInt(playerid,"ac_car_id",-1);
			KickEx(playerid);
		}
	}
	new Float:x,Float:y,Float:z;
	GetPlayerCameraFrontVector(playerid,x,y,z);
	if(floatcmp(1.0, floatabs(x))==-1 || floatcmp(1.0, floatabs(y))==-1 || floatcmp(1.0, floatabs(z))==-1)
	{
		if(FirstKick{playerid}) KickEx(playerid);
		else FirstKick{playerid}=1;
		return false;
	}
	if(GetPlayerWeapon(playerid) == 0 && gPlayerLogged[playerid] == 1 && !IsPlayerInAnyVehicle(playerid))
	if (PlayerInfo[playerid][pAdmInections] < 7 && GetPlayerState(playerid) != PLAYER_STATE_SPECTATING)
	{
		new gun1 = GetPlayerAmmo(playerid);
		if(gun1 > 3)
