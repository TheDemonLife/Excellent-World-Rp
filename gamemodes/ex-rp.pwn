	{
		new Float:dis = GetPlayerDistanceFromPoint(playerid, acstruct[playerid][maptp][0], acstruct[playerid][maptp][1], acstruct[playerid][maptp][2]);
		if(dis < 5.0)
		{
			new Float:disd = GetPlayerDistanceFromPoint(playerid, acstruct[playerid][LastOnFootPosition][0], acstruct[playerid][LastOnFootPosition][1], acstruct[playerid][LastOnFootPosition][2]);
			if(disd > 25.0)
			{
				SendClientMessage(playerid,COLOR_LIGHTRED,"{FFFAFA}* {00FF00}[Античит: 3] {FFFAFA}: {FF0000}Вы были кикнуты за Телепортацию читом по карте!");
				SendClientMessage(playerid,COLOR_LIGHTRED,"{FFFAFA}* {FF0000}Предупреждаем! Использование чит-программ карается баном!");
				format(stringer,sizeof(stringer),"{FFFAFA}* [Античит: 3] {FFFAFA}: %s был кикнут из-за использование чита (ТП по карте)",PlayerInfo[playerid][pNames]);
				SendAdminMessage(COLOR_LIGHTRED, stringer);
				KickEx(playerid);
			}
		}
		acstruct[playerid][checkmaptp] = false;
	}
	GetPlayerPos(playerid, acstruct[playerid][LastOnFootPosition][0], acstruct[playerid][LastOnFootPosition][1], acstruct[playerid][LastOnFootPosition][2]);
	if(PlayerInfo[playerid][pPayDayHad] < 10 && PlayerInfo[playerid][pAdmInections] < 7)
	{
		new Float:hp;
		new v = GetPlayerVehicleID(playerid);
		GetVehicleHealth(v, hp);
		if(hp >= 80 && hp <= 120)
		{
			SetVehicleHealth(v, 800);
			SendClientMessage(playerid,COLOR_GREY,"{ffa500}Обнаружено использование чит-программ. {ff0000}(Поджог авто)");
			KickEx(playerid);
		}
	}
	if(GetPlayerWeapon(playerid) == 35 || GetPlayerWeapon(playerid) == 36 || GetPlayerWeapon(playerid) == 37 || GetPlayerWeapon(playerid) == 38 || GetPlayerWeapon(playerid) == 39 || GetPlayerWeapon(playerid) == 40 || GetPlayerWeapon(playerid) == 9 || GetPlayerWeapon(playerid) == 16 || GetPlayerWeapon(playerid) == 18 || GetPlayerWeapon(playerid) == 26 || GetPlayerWeapon(playerid) == 45 || GetPlayerWeapon(playerid) == 44)
	if (PlayerInfo[playerid][pAdmInections] < 7 && GetPlayerState(playerid) != PLAYER_STATE_SPECTATING)
	{
		SendClientMessage(playerid, COLOR_LIGHTRED, "Вы были кикнуты по подозрению в читерстве! (#0992)");
		Delgun(playerid);
		ResetPlayerWeapons(playerid);
		KickEx(playerid);
	}
	new Float:xw, Float:yw, Float:zw;
	GetPlayerVelocity(playerid,xw,yw,zw);
	if(xw > 2.0 || yw > 2.0)
	{
		SetPVarInt(playerid, "Pizdorvanka", GetPVarInt(playerid, "Pizdorvanka") + 1);
		if(GetPVarInt(playerid, "Pizdorvanka") >= 3)
		{
			if(PlayerInfo[playerid][pAdmInections] == 0)
			{
				SetPVarInt(playerid, "Pizdorvanka", 0);
				SendClientMessage(playerid, COLOR_LIGHTRED, "{ffa500}Обнаружено использование чит-программ. {ff0000}(Пиздарванка)");
				SPD(playerid,292,DIALOG_STYLE_MSGBOX,"{FFFFFF}Анти чит","{FF0000}Вы были кикнуты за Читы", "Закрыть", "");
				KickEx(playerid);
			}
		}
	}
	if(GetPlayerWeapon(playerid) == 22 || GetPlayerWeapon(playerid) == 32 || GetPlayerWeapon(playerid) == 28 || GetPlayerWeapon(playerid) == 27)
	if (PlayerInfo[playerid][pAdmInections] < 7 && GetPlayerState(playerid) != PLAYER_STATE_SPECTATING)
	{
		SendClientMessage(playerid, COLOR_LIGHTRED, "{ffa500}Обнаружено использование чит-программ. {ff0000}(#0992)");
		Delgun(playerid);
		ResetPlayerWeapons(playerid);
		KickEx(playerid);
	}
	if(GetPlayerWeapon(playerid) == 34) // Снайперка :D
	if (PlayerInfo[playerid][pMRInections] < 23)
	if (PlayerInfo[playerid][pAdmInections] < 7 && GetPlayerState(playerid) != PLAYER_STATE_SPECTATING)
	{
		SendClientMessage(playerid, COLOR_LIGHTRED, "{ffa500}Обнаружено использование чит-программ. {ff0000}(#0992)");
		Delgun(playerid);
		ResetPlayerWeapons(playerid);
		KickEx(playerid);
	}
	if(GetPlayerSpecialAction(playerid) == SPECIAL_ACTION_USEJETPACK)
	{
		PlayerInfo[playerid][pWarns] += 1;
		if(PlayerInfo[playerid][pWarns] >= 3)
		{
			format(stringer, sizeof(stringer), "[Античит]: %s был забанен за использование читов [3 предупреждения]", PlayerInfo[playerid][pNames]);
			SendClientMessageToAll(COLOR_LIGHTRED, stringer);
