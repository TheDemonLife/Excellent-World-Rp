public OnPlayerUpdate(playerid)
{
    if(plafk[playerid] > -2)
    {
    if(plafk[playerid] > 0)
    {
    new string[128];
    format(string,sizeof(string),"{FF0000}Вы были на паузе: {0FF000}%s",ConvertSeconds(plafk[playerid]));
